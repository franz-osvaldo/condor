<div class="row">
  <div class="col-md-4 form_container">
    <div class="form_header text-center">
      <i class="fa fa-list-ol" aria-hidden="true"></i></i> Inspecciones
    </div>
    <%= render partial: 'form', locals: {inspection: @inspection} %>
  </div>
  <div class="col-md-8">
    <div class="table_container">
      <div class="row table_head">
        <div class="col-md-6">Nombre</div>
        <div class="col-md-6">Administrar</div>
      </div>
      <div class="nano">
        <div class="content">
          <%= render @inspections %>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="modal_container"></div>
<script>
  $(document).ready(function () {
    $(".nano").nanoScroller({ scroll: 'bottom' });
  });
</script>